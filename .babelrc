{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": true
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "targets": {
          "node": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-async-to-generator",
      {
        "targets": {
          "node": true
        }
      }
    ],
    [
      "@babel/plugin-syntax-dynamic-import",
      {
        "targets": {
          "node": true
        }
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "targets": {
          "node": true
        }
      }
    ],
    [
      "add-module-exports"
    ]
  ]
}